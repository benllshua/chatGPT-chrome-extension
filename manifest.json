{
  "manifest_version": 3,
  "name": "ChatGPT Search Results",
  "version": "1.0",
  "description": "This extension adds ChatGPT results to Google search results.",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
